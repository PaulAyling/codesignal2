    "def isMAC48Address(inputString):\n",
    "  r = inputString.split('-')\n",
    "  if len(r)!=6:\n",
    "    return 0\n",
    "  for x in r:\n",
    "    if len(x)!=2:\n",
    "      return 0\n",
    "    try:\n",
    "      int(x,16)\n",
    "    except:\n",
    "      return 0\n",
    "  return 1\n",
    "inputString='01-23-4z-67-89-AB'\n",
    "print(isMAC48Address(inputString))"
